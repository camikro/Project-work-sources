@article{shekhawatBioceramicCompositesOrthopaedic2021,
  title = {Bioceramic Composites for Orthopaedic Applications: {{A}} Comprehensive Review of Mechanical, Biological, and Microstructural Properties},
  shorttitle = {Bioceramic Composites for Orthopaedic Applications},
  author = {Shekhawat, Deepika and Singh, Amit and Banerjee, M. K. and Singh, Tej and Patnaik, Amar},
