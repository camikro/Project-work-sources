@article{shekhawatBioceramicCompositesOrthopaedic2021,
  title = {Bioceramic Composites for Orthopaedic Applications: {{A}} Comprehensive Review of Mechanical, Biological, and Microstructural Properties},
  shorttitle = {Bioceramic Composites for Orthopaedic Applications},
  author = {Shekhawat, Deepika and Singh, Amit and Banerjee, M. K. and Singh, Tej and Patnaik, Amar},
  year = 2021,
  month = feb,
  journal = {Ceramics International},
  volume = {47},
  number = {3},
  pages = {3013--3030},
  issn = {0272-8842},
  doi = {10.1016/j.ceramint.2020.09.214},
  urldate = {2025-11-23},
  abstract = {Bioceramics have been widely utilized for orthopaedic applications in which the biocompatibility and mechanical properties of the materials are vital characteristics to be considered for their clinical use. Till date, extensive studies have been devoted to developing a range of scientific ways for tailoring the microstructure of bioceramics in order to attain the trade-off of mechanical properties and biocompatibility of the final product. Owing to low reactivity, earlier stabilization and longer functional life of bioceramic, the developed implants are capable of replicating the mechanical behaviour of original bone. As the safety of the patient and its ultimate functionality are the ultimate goal of the selected implant material hence, the present literature survey investigates and brings forth the important aspects associated to the mechanical, biological and microstructural characteristics of bioceramics employed in orthopaedic applications. The review paper majorly focuses on effective utilization of various materials as an additive in bioceramics and processing techniques used for enhancement of properties, enabling the use of material in orthopaedic applications. The influence of various additives on the microstructure, mechanical properties and biological performance of developed bioceramics orthopaedic implants has been elaborately discussed. Furthermore, future prospects are proposed to promote further innovations in bioceramics research.},
  keywords = {Bioceramics,Biocompatibility,Mechanical properties,Microstructure,Orthopaedic implants},
  file = {/Users/camillakrogh/Zotero/storage/SJWTNXW7/Shekhawat et al. - 2021 - Bioceramic composites for orthopaedic applications A comprehensive review of mechanical, biological.pdf;/Users/camillakrogh/Zotero/storage/L5DK2UMQ/S0272884220329187.html}
}

@article{sonowalAdvancementsBioceramicsBiomedical2025,
  title = {Advancements of Bioceramics in Biomedical Applications},
  author = {Sonowal, Lidiya and Gautam, Sanjeev and Mambiri, Lillian Tsitsi and Depan, Dilip},
  year = 2025,
  month = oct,
  journal = {Next Materials},
  volume = {9},
  pages = {101010},
  issn = {2949-8228},
  doi = {10.1016/j.nxmate.2025.101010},
  urldate = {2025-11-23},
  abstract = {Bioceramics have become a key class of materials in contemporary medicine because of their remarkable mechanical strength, biocompatibility, and biological system-interaction capabilities. It has been used historically in dentistry and orthopedics, but new developments have extended its use into cutting-edge biomedical fields like drug delivery, bioimaging, regenerative medicine, and smart implants. With an emphasis on the classification, physicochemical characteristics, and multifunctional roles of bioceramics, this review provides a thorough overview of the field's present state and future prospects. Innovative developments in hybrid composite systems, additive manufacturing methods, and nanostructured ceramics that close the gap between artificial scaffolds and natural tissue architecture are given special attention. Furthermore, there is a critical analysis of the incorporation of bioceramics into stimuli-responsive systems and theranostic platforms. This article proposes strategic paths for future research by noting current constraints, including brittleness, limited load-bearing capacity, and difficulties in large-scale production. In addition to inspiring next-generation advancements in tissue engineering, personalized therapies, and diagnostic applications, the study seeks to offer a cohesive framework for comprehending the changing role of bioceramics in precision medicine.},
  keywords = {3D printing,Bioceramics,Biomedical imaging,Drug delivery,Orthopedic},
  file = {/Users/camillakrogh/Zotero/storage/84H8H6FC/Sonowal et al. - 2025 - Advancements of bioceramics in biomedical applications.pdf;/Users/camillakrogh/Zotero/storage/UEV74JV4/S2949822825005283.html}
}

